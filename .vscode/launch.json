    "version": "2.0.0",
    "configurations": [
        {
            "name": "Debug Current File",
            "type": "cppdbg",
            "request": "launch",
            // "program": "${workspaceFolder}/build/FractureMesh/dfnTest",
            "program": "${workspaceFolder}/../${workspaceFolderBasename}-build/${relativeFileDirname}/${fileBasenameNoExtension}",
            // "preLaunchTask": "build",
            "internalConsoleOptions": "openOnSessionStart",
            "logging": {
                "moduleLoad": false,
                "programOutput": true,
                "trace": false
            },
            "showDisplayString": false,
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false, // set true to enable keyboard input
            "osx": {
                "MIMode": "lldb"
            }
        }
    ]
}